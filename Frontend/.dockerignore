# .dockerignore

# Ignore node_modules
node_modules

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build outputs
dist
build

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore editor-specific files
*.swp
*.swo
*.idea
*.vscode
*.DS_Store

# Ignore Docker files
Dockerfile
docker-compose*

# Ignore test coverage
coverage

# Ignore lock files
package-lock.json
yarn.lock
